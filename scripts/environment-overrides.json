{
  "Development": {
    "MainConfig": {
      "ActivePlugins": [
        "PerformanceCounter",
        "SitesFilter",
        "GraphiteOut",
        "ConsoleOut"
      ]
    },
    "GraphiteOutConfig": {
      "Debug": true,
      "Hostname": "graphite.edops.myob.com",
      "Port" : "2010"
    },
    "SitesFilterConfig": {
      "Debug": true
    }
  },
  "Test": {
    "MainConfig": {
      "Interval": "5000",
      "ActivePlugins": [
        "PerformanceCounter",
        "SitesFilter",
        "GraphiteOut",
        "ConsoleOut"
      ]
      
    },
    "GraphiteOutConfig": {
      "Debug": true,
      "Hostname": "10.0.0.14",
      "Prefix": "advanced.development",
      "Port": "2003"
    },
    "SitesFilterConfig": {
      "Debug": true
    }
  },
  "MinTest": {
    "MainConfig": {
      "Host": "minimal_test_host",
      "Interval": "10000",
      "ActivePlugins": [
        "PerformanceCounter",
        "GraphiteOut",
        "ConsoleOut"
      ]
    },
    "GraphiteOutConfig": {
      "SendBufferSize": 2000,
      "Protocol": "tcp",
      "Servername": "minimal",
      "Debug": true,
      "Hostname": "10.0.0.14",
      "Prefix": "minimal.test",
      "Port": "2003"
    },
    "PerformanceCounterConfig": {
      "categories": [
        {
          "name": "Processor",
          "counters": [
            "% Processor Time"
          ],
          "instances": [
            "_total"
          ]
        },
        {
          "name": "Memory",
          "counters": [
            "Available MBytes"
          ]
        },
        {
          "name": "System",
          "counters": [
            "processes"
          ]
        }
      ]
    }
  },
  "Prod": {
    "MainConfig": {
      "Host": "unused_graphite_web_udp_hostname",
      "Interval": "10000",
      "ActivePlugins": [
        "PerformanceCounter",
        "SitesFilter",
        "GraphiteOut"
      ]
    },
    "GraphiteOutConfig": {
      "SendBufferSize": 2000,
      "Protocol": "tcp",
      "Servername": "preet.i-9999999",
      "Debug": false,
      "Hostname": "10.0.0.14",
      "Prefix": "advanced.production",
      "Port": "2003"
    },
    "SitesFilterConfig": {
      "Debug": false,
      "Categories": {
        "ASP.NET Applications": {
          "PreserveOriginal": false,
          "Filters": [
            "lmw3svc"
          ]
        },
        "Process": {
          "PreserveOriginal": false,
          "Filters": [
            "w3wp.process"
          ]
        },
        "ASP.NET": {
          "Filters": [
            "lmw3svc"
          ],
          "PreserveOriginal": false
        },
        ".NET CLR Exceptions": {
          "Filters": [
            "w3wp.net"
          ],
          "PreserveOriginal": false
        },
        ".NET CLR LocksAndThreads": {
          "Filters": [
            "w3wp.net"
          ],
          "PreserveOriginal": false
        },
        ".NET CLR Memory": {
          "Filters": [
            "w3wp.net"
          ],
          "PreserveOriginal": false
        },
        "W3SVC_W3WP": {
          "Filters": [
            "w3svc"
          ],
          "PreserveOriginal": false
        }
      }
    }
  }
